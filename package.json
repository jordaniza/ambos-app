		"@tailwindcss/typography": "0.5.9",
		"@typechain/ethers-v5": "^11.1.1",
		"@types/node": "20.5.7",
		"@types/qrcode": "^1.5.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vite-pwa/assets-generator": "^0.0.10",
		"@vite-pwa/sveltekit": "^0.2.7",
