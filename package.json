		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "8.4.29",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"rollup-plugin-polyfill-node": "^0.12.0",
		"stream-browserify": "^3.0.0",
		"svelte": "^4.0.5",
		"svelte-carousel": "^1.0.25",
		"svelte-check": "^3.4.3",
		"tailwindcss": "3.3.3",
		"tslib": "^2.4.1",
		"typechain": "^8.3.1",
